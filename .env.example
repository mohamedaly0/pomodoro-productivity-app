# Environment variables for development
# Copy this file to .env and fill in your values

# Todoist Integration
VITE_TODOIST_CLIENT_ID=your_todoist_client_id
VITE_TODOIST_REDIRECT_URI=http://localhost:5173/auth/todoist/callback

# Spotify Integration  
VITE_SPOTIFY_CLIENT_ID=your_spotify_client_id
VITE_SPOTIFY_REDIRECT_URI=http://localhost:5173/auth/spotify/callback

# Google Calendar Integration
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_GOOGLE_REDIRECT_URI=http://localhost:5173/auth/google/callback

# Microsoft Graph Integration (To Do, Planner, Calendar)
VITE_MS_CLIENT_ID=your_microsoft_client_id
VITE_MS_TENANT_ID=common
VITE_MS_REDIRECT_URI=http://localhost:5173/auth/microsoft/callback

# App Configuration
VITE_APP_NAME=PomoFocus
VITE_APP_VERSION=0.1.0
